@model List<ShinyTodoWebApp.Models.Todo>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <h1>
        This is my first todo
    </h1>

    <div>
        <ul>
            @{
                foreach (var listElement in Model)
                {
                    <li>
                        @listElement.Title
                    </li>
                }
            }
        </ul>
    </div>
</body>
</html>